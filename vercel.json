{
  "version": 2,
  "builds": [
    {
      "src": "server/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server/main.js"
    },
    {
      "src": "/(assets|images|favicon)/(.*)",
      "dest": "/$1/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}